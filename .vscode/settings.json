{
  "typescript.tsdk": "./node_modules/typescript/lib",
  "search.exclude": {
    "**/node_modules": true,
    "**/build": true,
    "**/.awcache": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/build": true,
    "**/.awcache": true
  },
  "editor.tabSize": 2,
  "files.trimTrailingWhitespace": true,
  "javascript.validate.enable": false,
  "eslint.enable": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "prettier.proseWrap": false,
  "editor.formatOnSave": true
}
